{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-view404.html",

    "src/my-dashboard.html",
    "src/my-accountlist.html",
    "src/my-accountlist-new.html",
    "src/my-about-us.html",
    
    "src/my-signup.html",
    "src/my-resetpw.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
